{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://maxicast96.github.io/A.U.R.O.R.A",
  "description": "A.U.R.O.R.A - Aplicación de gestión óptica",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n vite,ai -c blue,magenta \"vite\" \"python ..\\ai-chat-service\\main.py\"",
    "dev:setup": "npm run ai:install && concurrently -n vite,ai -c blue,magenta \"vite\" \"python ..\\ai-chat-service\\main.py\"",
    "dev:vite": "vite",
    "dev:ai": "python ..\\ai-chat-service\\main.py",
    "dev:all": "concurrently -n vite,ai -c blue,magenta \"vite\" \"python ..\\ai-chat-service\\main.py\"",
    "ai:install": "python -m pip install --upgrade pip && python -m pip install -r ..\\ai-chat-service\\requirements.txt",
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "animate.css": "^4.1.1",
    "axios": "^1.10.0",
    "framer-motion": "^12.16.0",
    "lightningcss": "^1.30.1",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "react-uploader": "^3.43.0",
    "recharts": "^3.1.0",
    "tailwindcss": "^4.1.8",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "rollup": "^4.46.3",
    "concurrently": "^9.1.2"
  }
}
